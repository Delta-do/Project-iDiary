<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.idiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/idiary_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/idiary_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.IDiary">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AddSubjectsActivity"
            android:label="AddSubjectsActivity"/>

        <activity
            android:name=".EditDayActivity"
            android:label="EditDayActivity"/>

    </application>

</manifest>